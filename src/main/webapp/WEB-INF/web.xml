		<servlet-class>com.revature.ProfileServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/Profile</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ReservationsServlet</servlet-name>
		<servlet-class>com.revature.ReservationsServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReservationsServlet</servlet-name>
		<url-pattern>/Reservations</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ReservationApprovalServlet</servlet-name>
		<servlet-class>com.revature.ReservationApprovalServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ReservationApprovalServlet</servlet-name>
		<url-pattern>/ReservationApproval</url-pattern>
	</servlet-mapping>
	
	<servlet>
